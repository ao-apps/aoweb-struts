        <ignoreVersion type="regex">[0-9]{8}.*</ignoreVersion>
      </ignoreVersions>
    </rule>
    <rule groupId="com.stripe" artifactId="stripe-java" comparisonMethod="maven">
      <ignoreVersions>
        <ignoreVersion type="regex">.*-beta\.[0-9]+</ignoreVersion>
      </ignoreVersions>
    </rule>
    <rule groupId="ognl" comparisonMethod="ognl">
      <ignoreVersions>
        <!-- TODO: When updating new than struts2-6.3.0.1 make sure this is correct to still ignore -->
        <ignoreVersion type="exact">3.4.0</ignoreVersion>
        <ignoreVersion type="exact">3.4.1</ignoreVersion>
        <ignoreVersion type="exact">3.4.2</ignoreVersion>
      </ignoreVersions>
    </rule>
    <rule groupId="org.apache.logging.log4" comparisonMethod="maven">
      <ignoreVersions>
        <ignoreVersion type="regex">.*-alpha[0-9]*</ignoreVersion>
      </ignoreVersions>
    </rule>
