<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE form-validation PUBLIC
     "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
          "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">
	  
<form-validation>
    <formset>
        <form name="ticketForm">
            <field property="accounting" depends="required">
                <msg name="required" key="TicketForm.field.accounting.required" />
            </field>
            <field property="summary" depends="required">
                <msg name="required" key="TicketForm.field.summary.required" />
            </field>
        </form>
    </formset>
</form-validation>
