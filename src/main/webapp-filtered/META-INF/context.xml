<!-- META-INF/context.xml is used for local development deployment only -->
<Context path="/aoweb-struts">
	<Parameter
		name="root.aoserv.client.username"
		value="@{aoweb-struts-webapp.root.aoserv.client.username}"
		override="false"
	/>
	<Parameter
		name="root.aoserv.client.password"
		value="@{aoweb-struts-webapp.root.aoserv.client.password}"
		override="false"
	/>
	<Parameter
		name="exception.showError"
		value="true"
		override="false"
	/>
	<Parameter
		name="com.aoindustries.website.SiteSettings.canEditResources"
		value="true"
		override="false"
	/>
</Context>
